{
  "agent": {
    "logfile": "/opt/aws/amazon-cloudwatch-agent/logs/amazon-cloudwatch-agent.log"
  },
  "logs": {
    "logs_collected": {
      "files": {
        "collect_list": [
          {
            "file_path": "/opt/aws/amazon-cloudwatch-agent/logs/amazon-cloudwatch-agent.log",
            "log_group_name": "/agent-integration-tests/",
            "log_stream_name": "{instance_id}_{hostname}",
            "timezone": "Local",
            "retention_in_days": 7
          },
          {
            "file_path": "/var/log/messages",
            "log_group_name": "/ec2/var/log/messages",
            "log_stream_name": "{instance_id}_{hostname}",
            "timezone": "Local",
            "retention_in_days": 7
          },
          {
            "file_path": "/var/log/secure",
            "log_group_name": "/ec2/var/log/secure",
            "log_stream_name": "{instance_id}_{hostname}",
            "timezone": "Local",
            "retention_in_days": 7
          },
          {
            "file_path": "/resim/agent.log",
            "log_group_name": "/resim",
            "log_stream_name": "{instance_id}_{hostname}",
            "timezone": "Local",
            "retention_in_days": 7
          }
        ]
      }
    },
    "log_stream_name": "/ec2/catchall"
  }
}
